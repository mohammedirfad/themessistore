      if(langify.settings.debug) {
        if(!data) {
          var data = '';
        }
        
        var css = 'color: green; font-weight: bold;';
        if(type === 'error') {
          css = 'color: red; font-weight: bold;';
        }

        console.log(`%c ${title}: \n`, css, data);
      }
    }
  };


